.order-container {
  position: relative;
  z-index: 1;
  padding: 30rpx;
}

.order-container .title {
  font-size: 28rpx;
}

.order-container .order-type-wrapper {
  padding: 20rpx 0;
}

.order-container .order-type-wrapper .tabs {
  position: relative;
  display: flex;
  align-items: flex-end;
}

.order-container .order-type-wrapper .tabs:after {
  content: '';
  position: absolute;
  background-color: #fff;
  width: 100%;
  height: 50%;
  bottom: 0;
  left: 0;
  z-index: 0;
}

.order-container .order-type-wrapper .tabs .left {
  border-radius: 25rpx 0 30rpx 0;
}

.order-container .order-type-wrapper .tabs .right {
  border-radius: 0 25rpx 0 30rpx;
}

.order-container .order-type-wrapper .tabs view {
  position: relative;
  flex: 5;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #febebe;
  color: #333;
  font-size: 28rpx;
  z-index: 1;
}

.order-container .order-type-wrapper .tabs view.active {
  background-color: #fff;
  flex: 6;
  height: 100rpx;
  border-radius: 25rpx 25rpx 0 0;
  color: #000;
  font-size: 30rpx;
}

.order-container .order-type-wrapper .tab-content-1, .order-container .order-type-wrapper .tab-content-2 {
  background-color: #fff;
  border-radius: 0 0 25rpx 25rpx;
  padding: 26rpx;
}

.order-container .order-type-wrapper .tab-content-1 .address-wrapper, .order-container .order-type-wrapper .tab-content-2 .address-wrapper {
  display: flex;
  align-items: center;
}

.order-container .order-type-wrapper .tab-content-1 .address-wrapper .address, .order-container .order-type-wrapper .tab-content-2 .address-wrapper .address {
  font-size: 32rpx;
  font-weight: bold;
  flex: 1;
}

.order-container .order-type-wrapper .tab-content-1 .address-wrapper .icon, .order-container .order-type-wrapper .tab-content-2 .address-wrapper .icon {
  margin-left: 30rpx;
  width: 42rpx;
  height: 42rpx;
}

.order-container .order-type-wrapper .tab-content-1 .phone, .order-container .order-type-wrapper .tab-content-2 .phone {
  color: #999;
  font-size: 26rpx;
  padding: 16rpx 0;
}

.order-container .order-type-wrapper .tab-content-1 .deliver, .order-container .order-type-wrapper .tab-content-2 .deliver {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx 0;
  margin-top: 10rpx;
  border-top: 1px solid #eee;
}

.order-container .order-type-wrapper .tab-content-1 .deliver .time, .order-container .order-type-wrapper .tab-content-2 .deliver .time {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 26rpx;
  color: #fe4a4b;
}

.order-container .order-type-wrapper .tab-content-1 .deliver .time .icon, .order-container .order-type-wrapper .tab-content-2 .deliver .time .icon {
  width: 38rpx;
  height: 38rpx;
}

.order-container .order-type-wrapper .tab-content-1 .picker {
  display: flex;
}

.order-container .order-type-wrapper .tab-content-1 .picker .area {
  flex: 1;
}

.order-container .order-type-wrapper .tab-content-1 .picker image {
  width: 40rpx;
  height: 40rpx;
}

.order-container .order-type-wrapper .tab-content-1 .delivery_type_1_input_wapper {
  display: flex;
}

.order-container .order-type-wrapper .tab-content-1 .delivery_type_1_input_wapper .delivery_type_1_input {
  flex: 1;
  padding: 30rpx 0;
  font-size: 31rpx;
}

.order-container .order-type-wrapper .tab-content-1 .placeholder {
  height: auto;
  color: #aaa;
}

.order-container .order-type-wrapper .tab-content-2 .address-wrapper .right {
  text-align: right;
}

.order-container .order-type-wrapper .tab-content-2 .address-wrapper .right .map {
  font-size: 24rpx;
  padding: 10rpx 0;
  color: #fe4a4b;
}

.order-container .order-type-wrapper .tab-content-2 .address-wrapper .right .distance {
  font-size: 24rpx;
  width: 180rpx;
  color: #999;
}

.order-container .order-type-wrapper .tab-content-2 .take-info {
  display: flex;
  padding: 30rpx 0;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left {
  padding-right: 50rpx;
  width: 240rpx;
  position: relative;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left:before {
  content: '';
  position: absolute;
  width: 2rpx;
  height: 50%;
  background-color: #eee;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left .flex-1 {
  flex: 1;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left .time-text {
  font-size: 26rpx;
  color: #999;
  padding-bottom: 10rpx;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left .time-num {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: bold;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .left .time-num .icon {
  width: 40rpx;
  height: 40rpx;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .right {
  padding-left: 50rpx;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .right .phone-text {
  font-size: 26rpx;
  color: #999;
  padding-bottom: 10rpx;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .right .phone-num {
  display: flex;
  font-size: 28rpx;
  font-weight: bold;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .right .phone-num input {
  flex: 1;
}

.order-container .order-type-wrapper .tab-content-2 .take-info .right .phone-num text {
  font-size: 24rpx;
  color: #fe4a4b;
}

.order-container .order-type-wrapper .tab-content-2 .agree {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  padding: 20rpx 0;
}

.order-container .order-type-wrapper .tab-content-2 .agree .checkbox {
  display: flex;
  align-items: center;
}

.order-container .order-type-wrapper .tab-content-2 .agree .checkbox .icon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 4rpx;
}

.order-container .order-type-wrapper .tab-content-2 .agree .view-agree {
  font-size: 26rpx;
  color: #fe4a4b;
}

.order-container .order-detail-wrapper {
  background-color: #fff;
  border-radius: 25rpx;
  padding: 26rpx;
  margin-bottom: 20rpx;
}

.order-container .order-detail-wrapper .price {
  font-weight: bold;
}

.order-container .order-detail-wrapper .order-detail-title {
  font-size: 28rpx;
}

.order-container .order-detail-wrapper .order-item {
  display: flex;
  padding: 20rpx 0;
}

.order-container .order-detail-wrapper .order-item > image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 10rpx;
}

.order-container .order-detail-wrapper .order-item .right {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 10rpx;
}

.order-container .order-detail-wrapper .order-item .title-wrapper {
  display: flex;
  font-size: 28rpx;
}

.order-container .order-detail-wrapper .order-item .title-wrapper .title {
  flex: 1;
}

.order-container .order-detail-wrapper .order-item .title-wrapper .price {
  font-size: 22rpx;
}

.order-container .order-detail-wrapper .order-item .title-wrapper .price text {
  font-size: 28rpx;
  padding-left: 4rpx;
}

.order-container .order-detail-wrapper .order-item .tags {
  font-size: 22rpx;
  color: #999;
}

.order-container .order-detail-wrapper .order-item .amount {
  flex: 1;
  display: flex;
  align-items: flex-end;
  color: #999;
  font-size: 28rpx;
}

.order-container .order-detail-wrapper .title-item {
  display: flex;
  font-size: 28rpx;
  align-items: center;
  padding: 20rpx 0;
}

.order-container .order-detail-wrapper .title-item .desc {
  flex: 1;
  display: flex;
  align-items: center;
}

.order-container .order-detail-wrapper .title-item .desc .icon {
  width: 40rpx;
  height: 30rpx;
}

.order-container .order-detail-wrapper .title-item .desc .juan {
  padding-left: 8rpx;
}

.order-container .order-detail-wrapper .title-item .desc .deliver {
  font-size: 24rpx;
  color: #fe4a4b;
}

.order-container .order-detail-wrapper .title-item .price {
  display: flex;
  align-items: center;
  font-size: 22rpx;
}

.order-container .order-detail-wrapper .title-item .price text {
  font-size: 28rpx;
  padding-left: 4rpx;
}

.order-container .order-detail-wrapper .title-item .price .icon {
  width: 40rpx;
  height: 40rpx;
  opacity: 0.5;
}

.order-container .order-detail-wrapper .title-item .red {
  color: #fe4a4b;
}

.order-container .order-detail-wrapper .title-item .right-align {
  position: relative;
  right: -10rpx;
}

.order-container .order-memo-wrapper {
  background-color: #fff;
  border-radius: 25rpx;
  padding: 26rpx;
  margin-bottom: 20rpx;
}

.order-container .order-memo-wrapper .memo-input {
  box-sizing: border-box;
  margin-top: 20rpx;
  width: 100%;
  background-color: #f5f5f5;
  border-radius: 10rpx;
  padding: 30rpx;
  font-size: 26rpx;
  height: 200rpx;
}

.order-container .placeholder {
  height: 140rpx;
}

.order-submit-wrapper {
  position: fixed;
  width: 100%;
  z-index: 99;
  bottom: 0;
}

.order-submit-wrapper .buttons {
  display: flex;
  height: 100rpx;
  width: 100%;
}

.order-submit-wrapper .buttons .summary {
  flex: 2;
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: #000;
  color: #fff;
  font-size: 24rpx;
  box-sizing: border-box;
}

.order-submit-wrapper .buttons .summary view {
  padding: 0 30rpx;
}

.order-submit-wrapper .buttons .summary .yuan {
  font-size: 46rpx;
}

.order-submit-wrapper .buttons .summary .fee {
  color: #aaa;
  font-size: 22rpx;
}

.order-submit-wrapper .buttons .summary .discount {
  color: #aaa;
  font-size: 22rpx;
}

.order-submit-wrapper .buttons .pay {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
  color: #fff;
  font-size: 28rpx;
  background-color: #fe4a4b;
}

.order-submit-wrapper .iphone {
  display: flex;
  height: 40rpx;
  width: 100%;
}

.order-submit-wrapper .iphone .summary {
  flex: 2;
  background-color: #000;
}

.order-submit-wrapper .iphone .pay {
  flex: 1;
  background-color: #fe4a4b;
}
